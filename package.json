{
  "name": "edudash-pro",
  "version": "1.0.26",
  "main": "index.js",
  "engines": {
    "node": "20.x",
    "npm": ">=9.0.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-chart-kit",
          "@picovoice/porcupine-react-native",
          "dotenv",
          "exceljs",
          "i18next-browser-languagedetector",
          "js-yaml",
          "sentry-expo",
          "supabase",
          "tslib",
          "react-native-webrtc",
          "@daily-co/react-native-daily-js",
          "@config-plugins/react-native-webrtc",
          "react-native-pdf",
          "react-native-blob-util",
          "react-native-background-timer",
          "react-native-incall-manager",
          "react-native-pip-android"
        ],
        "listUnknownPackages": false
      }
    },
    "install": {
      "exclude": [
        "react",
        "react-dom",
        "react-native",
        "@react-native-community/datetimepicker",
        "@types/react",
        "expo-auth-session"
      ]
    }
  },
  "scripts": {
    "postinstall": "patch-package",
    "install:fix": "bash scripts/install-with-esbuild-fix.sh",
    "ssot:generate": "node scripts/generate-dash-ai-ssot.mjs",
    "ssot:check": "node scripts/check-dash-ai-ssot.mjs",
    "docs:policy:check": "node ./scripts/docs-policy-check.js",
    "switch-to": "./scripts/switch-expo-project.sh",
    "eas:project": "./scripts/switch-eas-project.sh",
    "eas:old": "./scripts/switch-eas-project.sh old",
    "eas:new": "./scripts/switch-eas-project.sh new",
    "eas": "node scripts/eas-wrapper.mjs",
    "eas:build": "node scripts/eas-wrapper.mjs build",
    "ota": "NODE_OPTIONS=\"--max-old-space-size=8192\" node scripts/eas-wrapper.mjs update --branch production --auto --non-interactive && node scripts/notify-ota-update.mjs --release-type ota --platforms android,ios",
    "ota:mark1": "./scripts/switch-eas-project.sh old && NODE_OPTIONS=\"--max-old-space-size=8192\" node scripts/eas-wrapper.mjs update --channel production && node scripts/notify-ota-update.mjs --release-type ota --platforms android,ios",
    "ota:release8": "./scripts/switch-eas-project.sh new && NODE_OPTIONS=\"--max-old-space-size=8192\" node scripts/eas-wrapper.mjs update --channel production && node scripts/notify-ota-update.mjs --release-type ota --platforms android,ios",
    "ota:release8:check": "npm run test:pre-ota",
    "ota:release8:safe": "npm run test:pre-ota && npm run ota:release8",
    "ota:playstore": "NODE_OPTIONS=\"--max-old-space-size=8192\" node scripts/eas-wrapper.mjs update --branch production && node scripts/notify-ota-update.mjs --release-type ota --platforms android,ios",
    "start": "npx expo start --dev-client --host localhost",
    "start:log": "mkdir -p logs && NODE_OPTIONS=\"--max-old-space-size=8192\" npx expo start --dev-client --host localhost 2>&1 | tee logs/expo-dev.log",
    "start:clear": "npx expo start --dev-client --host localhost --clear",
    "open:android": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:8083 tcp:8083 && adb shell am start -n com.edudashpro/expo.modules.devlauncher.launcher.DevLauncherActivity -a android.intent.action.VIEW -d \"exp://expo-development-client/?url=http://127.0.0.1:8081\"",
    "dev:android": "npm run start:clear & sleep 5 && npm run open:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:dev": "cross-env WEB_PORT=8082 expo start --web --port 8082",
    "web:dev:log": "mkdir -p logs && cross-env WEB_PORT=8082 expo start --web --port 8082 2>&1 | tee logs/dev-server.log",
    "web:clear": "node -e \"['8081','19000','19001','19006','8082'].forEach(p=>{try{require('child_process').execSync('fuser -k '+p+'/tcp',{stdio:'ignore'})}catch(e){}});\" && rm -rf .expo .cache && find /tmp -maxdepth 1 -type d -name 'metro-*' -exec rm -rf {} + 2>/dev/null || true",
    "web:build": "expo export --platform web --output-dir dist --clear",
    "vercel-build": "npm run web:build && bash scripts/copy-locales.sh && node scripts/patch-manifest-link.js",
    "web:preview": "serve -s dist -l 4173",
    "web:prod": "expo start --web --no-dev --minify",
    "cache:clear": "bash scripts/clear-caches.sh",
    "ota:preview:android": "npm run cache:clear && cross-env NODE_OPTIONS=--max-old-space-size=8192 node scripts/eas-wrapper.mjs update --channel preview --platform android --clear-cache --message \"Preview: PayFast return deep link, tier refresh, Dash AI nav hide, promo pricing\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "benchmark:voice:our": "tsx scripts/voice/run-our-dictation-benchmark.ts",
    "benchmark:voice:fetch-probes": "tsx scripts/voice/fetch-dictation-probes.ts",
    "benchmark:voice:import-wispr": "tsx scripts/voice/import-wispr-headtohead.ts",
    "benchmark:voice:score": "tsx scripts/voice/score-dictation-parity.ts",
    "benchmark:voice:check": "tsx scripts/voice/check-benchmark-gates.ts",
    "benchmark:voice:full": "tsx scripts/voice/run-full-benchmark.ts",
    "typecheck": "NODE_OPTIONS=--max-old-space-size=8192 tsc --noEmit",
    "typecheck:strict": "NODE_OPTIONS=--max-old-space-size=8192 tsc -p tsconfig.strict.json --noEmit",
    "validate:progress-bars": "tsx scripts/validate-progress-bars.ts",
    "lint": "npm run validate:progress-bars && npm run check:expo-filesystem && eslint . --ext .ts,.tsx --max-warnings 200",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "check:file-sizes": "node scripts/check-file-sizes.mjs",
    "check:file-sizes:touched": "node scripts/check-file-sizes-touched.mjs",
    "check:dash-runtime-prereqs": "tsx scripts/check-dash-runtime-prereqs.ts",
    "check:expo-filesystem": "node scripts/check-expo-filesystem-usage.js",
    "check:backups": "bash scripts/scan-backups.sh",
    "check:todos": "bash scripts/scan-todos.sh",
    "check:console": "rg -n --hidden --glob '!node_modules/**' 'console\\.log\\('",
    "git:remote": "bash scripts/git-remote-target.sh",
    "verify:dash-caps-contracts": "tsx scripts/acceptance/verify-dash-caps-contracts.ts",
    "verify:read-receipts-rollout": "tsx scripts/acceptance/verify-read-receipts-rollout.ts",
    "verify:reliability-audit": "npm run verify:dash-caps-contracts && npm run verify:read-receipts-rollout",
    "test:pre-ota": "npm run lint && npm run typecheck && npm test && npm run verify:reliability-audit && npm run check:expo-filesystem",
    "inspect-db": "tsx scripts/inspect-remote-db.ts",
    "inspect-db-full": "tsx scripts/inspect-with-service-role.ts",
    "setup-rls": "tsx scripts/apply-rls-setup.ts",
    "reset-migrations": "tsx scripts/reset-migration-history.ts",
    "lint:sql": "./lint-sql.sh lint",
    "fix:sql": "./lint-sql.sh fix",
    "sql:rules": "./lint-sql.sh rules",
    "sql:config": "./lint-sql.sh config",
    "fix-auth": "node scripts/fix-auth.js",
    "i18n:export": "node scripts/export-for-translation.js",
    "i18n:verify": "node scripts/verify-translations.js",
    "i18n:sweep": "node scripts/i18n-sweep.js --strict",
    "i18n:sweep:apply": "node scripts/i18n-sweep.js --apply --strict",
    "i18n:sweep:report": "node scripts/i18n-sweep.js",
    "build:android:apk": "node scripts/eas-wrapper.mjs build --platform android --profile production-apk --local",
    "build:android:aab": "node scripts/bump-android-version.mjs && node scripts/eas-wrapper.mjs build --platform android --profile production --clear-cache --non-interactive",
    "build:android:preview": "node scripts/eas-wrapper.mjs build --platform android --profile preview --local",
    "build:dev": "node scripts/eas-wrapper.mjs build --platform android --profile development",
    "build:dev:apk": "node scripts/eas-wrapper.mjs build --platform android --profile development-apk",
    "build:dev:local": "node scripts/eas-wrapper.mjs build --platform android --profile development-apk --local",
    "build:playstore": "node scripts/bump-android-version.mjs && EAS_NO_PROJECT_PROMPT=1 node scripts/eas-wrapper.mjs build --platform android --profile playstore --non-interactive",
    "build:playstore:nobump": "EAS_NO_PROJECT_PROMPT=1 node scripts/eas-wrapper.mjs build --platform android --profile playstore --non-interactive",
    "eas:project:list": "node scripts/eas-project.mjs list",
    "eas:project:current": "node scripts/eas-project.mjs current",
    "eas:project:use": "node scripts/eas-project.mjs use",
    "eas-build-pre-install": "echo 'EAS Build: Pre-install hook running'",
    "verify:prod": "npm run lint && npm run typecheck && npm test && npm run verify:reliability-audit && npm run check:expo-filesystem",
    "eas-build-post-install": "npm run typecheck",
    "eas-build-on-success": "echo 'EAS Build: Build completed successfully'",
    "eas-build-on-error": "echo 'EAS Build: Build failed'",
    "eas-build-on-complete": "echo 'EAS Build: Process completed'"
  },
