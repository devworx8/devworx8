{
  "expo": {
    "name": "EduDashPro",
    "slug": "comedudashproapp",
    "owner": "dash-ts-organization",
    "version": "1.0.23",
    "runtimeVersion": "1.0.23",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "edudashpro",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/**/*",
      "locales/**/*.json"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0f"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.k1ngdevops.edudashpro",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to your microphone so Dash can hear your questions and provide voice responses.",
        "NSCameraUsageDescription": "This app needs camera access so you can take photos to share with Dash.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can share images with Dash.",
        "NSLocationWhenInUseUsageDescription": "Allow EduDash Pro to access your location to match you with schools nearby.",
        "UIBackgroundModes": [
          "audio",
          "voip",
          "background-processing",
          "background-fetch",
          "fetch"
        ],
        "NSSpeechRecognitionUsageDescription": "Allow $(PRODUCT_NAME) to use speech recognition.",
        "LSApplicationQueriesSchemes": [
          "fnbbanking",
          "fnb",
          "standardbank",
          "absabanking",
          "absa",
          "nedbankmoneyapp",
          "nedbank",
          "capitecbank",
          "capitec",
          "tymebank",
          "discoverybank",
          "discovery",
          "investec",
          "africanbank"
        ]
      },
      "bitcode": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.edudashpro.app",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.BLUETOOTH",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_PHONE_CALL",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "android.permission.FOREGROUND_SERVICE_MICROPHONE",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PROJECTION",
        "android.permission.MANAGE_OWN_CALLS",
        "android.permission.READ_PHONE_STATE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.USE_FULL_SCREEN_INTENT",
        "android.permission.DISABLE_KEYGUARD",
        "android.permission.TURN_SCREEN_ON"
      ],
      "blockedPermissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ],
      "versionCode": 38,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.edudashpro.org.za",
              "pathPrefix": "/landing"
            },
            {
              "scheme": "https",
              "host": "edudashpro.org.za",
              "pathPrefix": "/landing"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.edudashpro.org.za",
              "pathPrefix": "/auth-callback"
            },
            {
              "scheme": "https",
              "host": "edudashpro.org.za",
              "pathPrefix": "/auth-callback"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "www.edudashpro.org.za",
              "pathPrefix": "/invite"
            },
            {
              "scheme": "https",
              "host": "edudashpro.org.za",
              "pathPrefix": "/invite"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      "expo-web-browser",
      "sentry-expo",
      [
        "expo-image-picker",
        {
          "photosPermission": "EduDash Pro needs access to your photos to share images with Dash.",
          "cameraPermission": "EduDash Pro needs camera access to take photos to share with Dash."
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access the microphone for voice messages.",
          "androidAudioForegroundServiceConfig": {
            "name": "Voice Message",
            "text": "Playing voice message..."
          }
        }
      ],
      "expo-speech-recognition",
      "expo-video",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-2808416461095370~5255516826",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      "expo-localization",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#00f5ff",
          "mode": "production",
          "androidMode": "default",
          "iosDisplayInForeground": true,
          "defaultChannel": "default"
        }
      ],
      [
        "@config-plugins/react-native-webrtc",
        {
          "cameraPermission": "EduDash Pro needs camera access for video calls with teachers and parents",
          "microphonePermission": "EduDash Pro needs microphone access for voice and video calls"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "./plugins/withAndroidManifestFix.js",
      "./plugins/withForegroundService.js",
      "./plugins/withFirebaseMessaging.js",
      "./plugins/withCallSounds.js",
      "./plugins/withAndroid15BootFix.js"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "EduDash Pro - AI-Powered Educational Platform",
      "shortName": "EduDash Pro",
      "lang": "en",
      "scope": "/",
      "themeColor": "#00f5ff",
      "backgroundColor": "#0a0a0f",
      "description": "Revolutionary AI-powered educational platform for preschools. Trusted by educators worldwide for next-generation learning experiences with Society 5.0 technology.",
      "keywords": [
        "education",
        "preschool",
        "AI",
        "learning",
        "teachers",
        "parents",
        "lessons",
        "artificial intelligence",
        "educational technology",
        "edtech"
      ],
      "author": "EduDash Pro Team",
      "viewport": "width=device-width, initial-scale=1, shrink-to-fit=no",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "any",
      "bundler": "metro",
      "output": "single",
      "build": {
        "babel": {
          "include": [
            "@expo/vector-icons"
          ]
        }
      },
      "serviceWorker": {
        "enabled": true
      }
    },
